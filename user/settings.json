{
  "colorscheme": "jetjbp",
  "restore": true,
  "conditionals": true,

  "modules": {
    "ask-on-quit": true,
    "folds": true,
    "gutter": true,
    "netrw": true,
    "notifications": false,
    "show-insert-mark": true,
    "skeletons": true,
    "statusline": true,
    "strict-cursor": "<Esc><Esc>"
  },

  "opts": {
    "breakindent": true,
    "clipboard": "unnamedplus",
    "cmdheight": 0,
    "conceallevel": 2,
    "confirm": true,
    "cursorcolumn": true,
    "cursorline": true,
    "diffopt": "foldcolumn:0,hiddenoff,vertical",
    "expandtab": true,
    "grepprg": "rg --hidden --no-heading --vimgrep",
    "ignorecase": true,
    "inccommand": "split",
    "infercase": true,
    "joinspaces": true,
    "lazyredraw": true,
    "list": true,
    "matchpairs": "(:),{:},[:],<:>,=:;",
    "mousefocus": true,
    "numberwidth": 1,
    "pumblend": 0,
    "pumheight": 6,
    "scrolloff": 9,
    "secure": true,
    "shiftround": true,
    "shortmess": "oOstIFS",
    "showbreak": "â†ª",
    "showmode": false,
    "showtabline": 0,
    "sidescrolloff": 9,
    "signcolumn": "number",
    "smartcase": true,
    "smartindent": true,
    "softtabstop": -1,
    "spelllang": "en,cjk",
    "spelloptions": "camel",
    "spellsuggest": "best",
    "splitbelow": true,
    "splitright": true,
    "swapfile": false,
    "synmaxcol": 160,
    "textwidth": 80,
    "timeoutlen": 300,
    "undofile": true,
    "undolevels": 6000,
    "updatetime": 30,
    "virtualedit": "all",
    "whichwrap": "<,>,[,],h,l,b,s,~",
    "winblend": 0,
    "winhighlight": "Normal:ActiveWindow,NormalNC:InactiveWindow",
    "wrap": false
  }
}
